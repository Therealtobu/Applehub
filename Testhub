-- Mining Category
createCategory({
    Name = "â›ï¸ Mining",
    Icon = "â›ï¸",
    Description = "Auto mine ores and teleport",
    OnOpen = function(parent)
        -- Ore Multi-Select Dropdown
        local oreDropdown = createMultiDropdown(parent, {
            Name = "Select Ores to Mine",
            Icon = "ðŸ’Ž",
            OnChanged = function(selected)
                table.clear(targetOreList)
                for ore, enabled in pairs(selected) do
                    if enabled then
                        table.insert(targetOreList, ore)
                    end
                end
            end
        })
        
        -- Initialize with current ores
        task.spawn(function()
            resetRockList()
            task.wait(0.1)
            if #nameOreList > 0 then
                oreDropdown.SetValues(nameOreList)
            end
        end)
        
        createButton(parent, {
            Name = "ðŸ”„ Refresh Ore List",
            Callback = function()
                resetRockList()
                task.wait(0.1)
                if #nameOreList > 0 then
                    oreDropdown.SetValues(nameOreList)
                    print("âœ… Ore list refreshed! Found " .. #nameOreList .. " ores")
                else
                    print("âš ï¸ No ores found nearby")
                end
            end
        })
        
        createToggle(parent, {
            Name = "Auto Mine",
            Icon = "âš’ï¸",
            Description = "Automatically mine selected ores",
            Default = false,
            Callback = function(value)
                isMine = value
            end
        })
        
        createToggle(parent, {
            Name = "Auto Farm Ores",
            Icon = "ðŸ’Ž",
            Description = "TP to selected ores and mine",
            Default = false,
            Callback = function(value)
                isFarm = value
                if value then
                    task.spawn(autoMine)
                end
            end
        })
        
        createToggle(parent, {
            Name = "Auto Teleport Locations",
            Icon = "ðŸ”„",
            Description = "Teleport across saved locations",
            Default = false,
            Callback = function(value)
                isTeleportFarm = value
                if value then
                    task.spawn(autoTeleportFarm)
                end
            end
        })
        
        createButton(parent, {
            Name = "ðŸ“ Add Current Location",
            Callback = function()
                addLocation()
                Library:SaveConfig("TigerHubTF/locationList.json", locationList)
                print("âœ… Location saved!")
            end
        })
        
        createButton(parent, {
            Name = "ðŸ—‘ï¸ Clear All Locations",
            Callback = function()
                table.clear(locationList)
                Library:SaveConfig("TigerHubTF/locationList.json", locationList)
                print("âœ… All locations cleared!")
            end
        })
    end
})

