-- ============================================
-- UI FUNCTIONS
-- ============================================
local function openUI()
    UIOpen = true
    Dashboard.Visible = true
    Dashboard.Size = UDim2.new(0, 0, 0, 0)
    
    TweenService:Create(Dashboard, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 380, 0, 450)
    }):Play()
    
    TweenService:Create(ToggleButton, TweenInfo.new(0.3), {
        Position = UDim2.new(0, -80, 0.5, -30)
    }):Play()
end

local function closeUI()
    UIOpen = false
    
    TweenService:Create(Dashboard, TweenInfo.new(0.35, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
        Size = UDim2.new(0, 0, 0, 0)
    }):Play()
    
    TweenService:Create(ToggleButton, TweenInfo.new(0.3), {
        Position = UDim2.new(0, 20, 0.5, -30)
    }):Play()
    
    task.wait(0.35)
    Dashboard.Visible = false
    Dashboard.Size = UDim2.new(0, 380, 0, 450)
end

ToggleButton.MouseButton1Click:Connect(function()
    if UIOpen then
        closeUI()
    else
        openUI()
    end
end)

CloseBtn.MouseButton1Click:Connect(function()
    closeUI()
end)

-- ============================================
-- CATEGORY & FEATURE CREATORS
-- ============================================
local function createCategory(config)
    local Card = Instance.new("TextButton")
    Card.Size = UDim2.new(1, 0, 0, 70)
    Card.BackgroundColor3 = CurrentTheme.CardBG
    Card.BackgroundTransparency = 0.2
    Card.BorderSizePixel = 0
    Card.Text = ""
    Card.AutoButtonColor = false
    Card.ZIndex = 6
    Card.Parent = CategoryScroll

    local CardCorner = Instance.new("UICorner")
    CardCorner.CornerRadius = UDim.new(0, 18)
    CardCorner.Parent = Card

    -- Glass layers
    local CardGlass1 = Instance.new("Frame")
    CardGlass1.Size = UDim2.new(1, 0, 1, 0)
    CardGlass1.BackgroundColor3 = CurrentTheme.Glass1
    CardGlass1.BackgroundTransparency = 0.7
    CardGlass1.BorderSizePixel = 0
    CardGlass1.ZIndex = 7
    CardGlass1.Parent = Card

    local CardGlass1Corner = Instance.new("UICorner")
    CardGlass1Corner.CornerRadius = UDim.new(0, 18)
    CardGlass1Corner.Parent = CardGlass1

    local CardBorder = Instance.new("UIStroke")
    CardBorder.Color = CurrentTheme.Border
    CardBorder.Thickness = 1
    CardBorder.Transparency = 0.5
    CardBorder.Parent = Card

    -- Icon BG
    local IconBG = Instance.new("Frame")
    IconBG.Size = UDim2.new(0, 44, 0, 44)
    IconBG.Position = UDim2.new(0, 13, 0.5, -22)
    IconBG.BackgroundColor3 = CurrentTheme.IconBG
    IconBG.BackgroundTransparency = 0
    IconBG.BorderSizePixel = 0
    IconBG.ZIndex = 8
    IconBG.Parent = Card

    local IconBGCorner = Instance.new("UICorner")
    IconBGCorner.CornerRadius = UDim.new(1, 0)
    IconBGCorner.Parent = IconBG

    -- Icon text
    local Icon = Instance.new("TextLabel")
    Icon.Size = UDim2.new(1, 0, 1, 0)
    Icon.BackgroundTransparency = 1
    Icon.Text = config.Icon or "âš™ï¸"
    Icon.Font = Enum.Font.GothamBold
    Icon.TextSize = 24
    Icon.TextColor3 = CurrentTheme.Icon
    Icon.ZIndex = 9
    Icon.Parent = IconBG

    -- Title
    local CardTitle = Instance.new("TextLabel")
    CardTitle.Size = UDim2.new(1, -130, 0, 22)
    CardTitle.Position = UDim2.new(0, 65, 0, 15)
    CardTitle.BackgroundTransparency = 1
    CardTitle.Text = config.Name
    CardTitle.Font = Enum.Font.GothamBold
    CardTitle.TextSize = 16
    CardTitle.TextColor3 = CurrentTheme.Text
    CardTitle.TextXAlignment = Enum.TextXAlignment.Left
    CardTitle.ZIndex = 8
    CardTitle.Parent = Card

    -- Description
    local Desc = Instance.new("TextLabel")
    Desc.Size = UDim2.new(1, -140, 0, 16)
    Desc.Position = UDim2.new(0, 65, 0, 39)
    Desc.BackgroundTransparency = 1
    Desc.Text = config.Description or ""
    Desc.Font = Enum.Font.Gotham
    Desc.TextSize = 12
    Desc.TextColor3 = CurrentTheme.SubText
    Desc.TextXAlignment = Enum.TextXAlignment.Left
    Desc.TextWrapped = true
    Desc.ZIndex = 8
    Desc.Parent = Card

    -- Chevron
    local Chevron = Instance.new("TextLabel")
    Chevron.Size = UDim2.new(0, 22, 0, 22)
    Chevron.Position = UDim2.new(1, -30, 0.5, -11)
    Chevron.BackgroundTransparency = 1
    Chevron.Text = "â€º"
    Chevron.Font = Enum.Font.GothamBold
    Chevron.TextSize = 24
    Chevron.TextColor3 = CurrentTheme.SubText
    Chevron.ZIndex = 8
    Chevron.Parent = Card

    Card.MouseButton1Click:Connect(function()
        TweenService:Create(CategoryScroll, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Position = UDim2.new(-1.2, 0, 0, 56)
        }):Play()
        
        FeatureWindow.Visible = true
        FeatureWindow.Position = UDim2.new(1.2, 0, 0, 56)
        WinTitle.Text = config.Name
        
        TweenService:Create(FeatureWindow, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
            Position = UDim2.new(0, 12, 0, 56)
        }):Play()
        
        for _, child in pairs(FeatureScroll:GetChildren()) do
            if child:IsA("Frame") or child:IsA("TextButton") then
                child:Destroy()
            end
        end
        
        if config.OnOpen then
            config.OnOpen(FeatureScroll)
        end
    end)

    Card.MouseEnter:Connect(function()
        TweenService:Create(Card, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
            BackgroundTransparency = 0.1
        }):Play()
        TweenService:Create(CardBorder, TweenInfo.new(0.2), {
            Transparency = 0.3
        }):Play()
    end)

    Card.MouseLeave:Connect(function()
        TweenService:Create(Card, TweenInfo.new(0.2, Enum.EasingStyle.Quad), {
            BackgroundTransparency = 0.2
        }):Play()
        TweenService:Create(CardBorder, TweenInfo.new(0.2), {
            Transparency = 0.5
        }):Play()
    end)
end

local function createToggle(parent, config)
    local Item = Instance.new("Frame")
    Item.Size = UDim2.new(1, 0, 0, 56)
    Item.BackgroundColor3 = CurrentTheme.CardBG
    Item.BackgroundTransparency = 0.2
    Item.BorderSizePixel = 0
    Item.ZIndex = 14
    Item.Parent = parent

    local ItemCorner = Instance.new("UICorner")
    ItemCorner.CornerRadius = UDim.new(0, 16)
    ItemCorner.Parent = Item

    -- Glass layers
    local ItemGlass1 = Instance.new("Frame")
    ItemGlass1.Size = UDim2.new(1, 0, 1, 0)
    ItemGlass1.BackgroundColor3 = CurrentTheme.Glass1
    ItemGlass1.BackgroundTransparency = 0.7
    ItemGlass1.BorderSizePixel = 0
    ItemGlass1.ZIndex = 15
    ItemGlass1.Parent = Item

    local ItemGlass1Corner = Instance.new("UICorner")
    ItemGlass1Corner.CornerRadius = UDim.new(0, 16)
    ItemGlass1Corner.Parent = ItemGlass1

    local ItemBorder = Instance.new("UIStroke")
    ItemBorder.Color = CurrentTheme.Border
    ItemBorder.Thickness = 0.5
    ItemBorder.Transparency = 0.6
    ItemBorder.Parent = Item

    -- Icon
    local IconLabel = Instance.new("TextLabel")
    IconLabel.Size = UDim2.new(0, 40, 1, 0)
    IconLabel.Position = UDim2.new(0, 0, 0, 0)
    IconLabel.BackgroundTransparency = 1
    IconLabel.Text = config.Icon or "âš™ï¸"
    IconLabel.Font = Enum.Font.GothamBold
    IconLabel.TextSize = 20
    IconLabel.TextColor3 = CurrentTheme.Text
    IconLabel.ZIndex = 16
    IconLabel.Parent = Item

    -- Name
    local NameLabel = Instance.new("TextLabel")
    NameLabel.Size = UDim2.new(1, -120, 0, 22)
    NameLabel.Position = UDim2.new(0, 44, 0, 10)
    NameLabel.BackgroundTransparency = 1
    NameLabel.Text = config.Name
    NameLabel.Font = Enum.Font.GothamSemibold
    NameLabel.TextSize = 15
    NameLabel.TextColor3 = CurrentTheme.Text
    NameLabel.TextXAlignment = Enum.TextXAlignment.Left
    NameLabel.ZIndex = 16
    NameLabel.Parent = Item

    -- Description
    local Desc = Instance.new("TextLabel")
    Desc.Size = UDim2.new(1, -125, 0, 14)
    Desc.Position = UDim2.new(0, 44, 0, 32)
    Desc.BackgroundTransparency = 1
    Desc.Text = config.Description or ""
    Desc.Font = Enum.Font.Gotham
    Desc.TextSize = 11
    Desc.TextColor3 = CurrentTheme.SubText
    Desc.TextXAlignment = Enum.TextXAlignment.Left
    Desc.ZIndex = 16
    Desc.Parent = Item

    -- Toggle
    local ToggleBtn = Instance.new("TextButton")
    ToggleBtn.Size = UDim2.new(0, 51, 0, 31)
    ToggleBtn.Position = UDim2.new(1, -60, 0.5, -15.5)
    ToggleBtn.BackgroundColor3 = CurrentTheme.ToggleOff
    ToggleBtn.BackgroundTransparency = 0
    ToggleBtn.BorderSizePixel = 0
    ToggleBtn.Text = ""
    ToggleBtn.AutoButtonColor = false
    ToggleBtn.ZIndex = 16
    ToggleBtn.Parent = Item

    local ToggleCorner = Instance.new("UICorner")
    ToggleCorner.CornerRadius = UDim.new(1, 0)
    ToggleCorner.Parent = ToggleBtn

    local Circle = Instance.new("Frame")
    Circle.Size = UDim2.new(0, 27, 0, 27)
    Circle.Position = UDim2.new(0, 2, 0.5, -13.5)
    Circle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Circle.BorderSizePixel = 0
    Circle.ZIndex = 17
    Circle.Parent = ToggleBtn

    local CircleCorner = Instance.new("UICorner")
    CircleCorner.CornerRadius = UDim.new(1, 0)
    CircleCorner.Parent = Circle

    local enabled = config.Default or false

    if enabled then
        ToggleBtn.BackgroundColor3 = CurrentTheme.ToggleOn
        Circle.Position = UDim2.new(1, -29, 0.5, -13.5)
    end

    ToggleBtn.MouseButton1Click:Connect(function()
        enabled = not enabled

        if enabled then
            TweenService:Create(ToggleBtn, TweenInfo.new(0.25, Enum.EasingStyle.Sine), {
                BackgroundColor3 = CurrentTheme.ToggleOn
            }):Play()
            TweenService:Create(Circle, TweenInfo.new(0.3, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
                Position = UDim2.new(1, -29, 0.5, -13.5)
            }):Play()
        else
            TweenService:Create(ToggleBtn, TweenInfo.new(0.25, Enum.EasingStyle.Sine), {
                BackgroundColor3 = CurrentTheme.ToggleOff
            }):Play()
            TweenService:Create(Circle, TweenInfo.new(0.3, Enum.EasingStyle.Cubic, Enum.EasingDirection.Out), {
                Position = UDim2.new(0, 2, 0.5, -13.5)
            }):Play()
        end

        if config.Callback then
            config.Callback(enabled)
        end
    end)
end

local function createButton(parent, config)
    local Btn = Instance.new("TextButton")
    Btn.Size = UDim2.new(1, 0, 0, 50)
    Btn.BackgroundColor3 = CurrentTheme.CardBG
    Btn.BackgroundTransparency = 0.2
    Btn.BorderSizePixel = 0
    Btn.Text = ""
    Btn.AutoButtonColor = false
    Btn.ZIndex = 14
    Btn.Parent = parent

    local BtnCorner = Instance.new("UICorner")
    BtnCorner.CornerRadius = UDim.new(0, 14)
    BtnCorner.Parent = Btn

    local BtnGlass = Instance.new("Frame")
    BtnGlass.Size = UDim2.new(1, 0, 1, 0)
    BtnGlass.BackgroundColor3 = CurrentTheme.Glass1
    BtnGlass.BackgroundTransparency = 0.7
    BtnGlass.BorderSizePixel = 0
    BtnGlass.ZIndex = 15
    BtnGlass.Parent = Btn

    local BtnGlassCorner = Instance.new("UICorner")
    BtnGlassCorner.CornerRadius = UDim.new(0, 14)
    BtnGlassCorner.Parent = BtnGlass

    local BtnBorder = Instance.new("UIStroke")
    BtnBorder.Color = CurrentTheme.Border
    BtnBorder.Thickness = 0.5
    BtnBorder.Transparency = 0.6
    BtnBorder.Parent = Btn

    local BtnLabel = Instance.new("TextLabel")
    BtnLabel.Size = UDim2.new(1, -20, 1, 0)
    BtnLabel.Position = UDim2.new(0, 10, 0, 0)
    BtnLabel.BackgroundTransparency = 1
    BtnLabel.Text = config.Name
    BtnLabel.Font = Enum.Font.GothamSemibold
    BtnLabel.TextSize = 15
    BtnLabel.TextColor3 = CurrentTheme.Text
    BtnLabel.TextXAlignment = Enum.TextXAlignment.Left
    BtnLabel.ZIndex = 16
    BtnLabel.Parent = Btn

    Btn.MouseButton1Click:Connect(function()
        if config.Callback then
            config.Callback()
        end
    end)

    Btn.MouseEnter:Connect(function()
        TweenService:Create(Btn, TweenInfo.new(0.2), {
            BackgroundTransparency = 0.1
        }):Play()
    end)

    Btn.MouseLeave:Connect(function()
        TweenService:Create(Btn, TweenInfo.new(0.2), {
            BackgroundTransparency = 0.2
        }):Play()
    end)
end

local function createMultiDropdown(parent, config)
    local selectedValues = {}
    local isOpen = false
    
    -- Main Container
    local Container = Instance.new("Frame")
    Container.Size = UDim2.new(1, 0, 0, 56)
    Container.BackgroundColor3 = CurrentTheme.CardBG
    Container.BackgroundTransparency = 0.2
    Container.BorderSizePixel = 0
    Container.ZIndex = 14
    Container.Parent = parent

    local ContCorner = Instance.new("UICorner")
    ContCorner.CornerRadius = UDim.new(0, 16)
    ContCorner.Parent = Container

    local ContGlass = Instance.new("Frame")
    ContGlass.Size = UDim2.new(1, 0, 1, 0)
    ContGlass.BackgroundColor3 = CurrentTheme.Glass1
    ContGlass.BackgroundTransparency = 0.7
    ContGlass.BorderSizePixel = 0
    ContGlass.ZIndex = 15
    ContGlass.Parent = Container

    local ContGlassCorner = Instance.new("UICorner")
    ContGlassCorner.CornerRadius = UDim.new(0, 16)
    ContGlassCorner.Parent = ContGlass

    local ContBorder = Instance.new("UIStroke")
    ContBorder.Color = CurrentTheme.Border
    ContBorder.Thickness = 0.5
    ContBorder.Transparency = 0.6
    ContBorder.Parent = Container

    -- Header Button
    local HeaderBtn = Instance.new("TextButton")
    HeaderBtn.Size = UDim2.new(1, 0, 0, 56)
    HeaderBtn.BackgroundTransparency = 1
    HeaderBtn.Text = ""
    HeaderBtn.ZIndex = 16
    HeaderBtn.Parent = Container

    local IconLabel = Instance.new("TextLabel")
    IconLabel.Size = UDim2.new(0, 40, 1, 0)
    IconLabel.Position = UDim2.new(0, 0, 0, 0)
    IconLabel.BackgroundTransparency = 1
    IconLabel.Text = config.Icon or "ðŸ“‹"
    IconLabel.Font = Enum.Font.GothamBold
    IconLabel.TextSize = 20
    IconLabel.TextColor3 = CurrentTheme.Text
    IconLabel.ZIndex = 17
    IconLabel.Parent = HeaderBtn

    local DropLabel = Instance.new("TextLabel")
    DropLabel.Size = UDim2.new(1, -100, 0, 22)
    DropLabel.Position = UDim2.new(0, 44, 0, 10)
    DropLabel.BackgroundTransparency = 1
    DropLabel.Text = config.Name
    DropLabel.Font = Enum.Font.GothamSemibold
    DropLabel.TextSize = 15
    DropLabel.TextColor3 = CurrentTheme.Text
    DropLabel.TextXAlignment = Enum.TextXAlignment.Left
    DropLabel.ZIndex = 17
    DropLabel.Parent = HeaderBtn

    local CountLabel = Instance.new("TextLabel")
    CountLabel.Size = UDim2.new(1, -100, 0, 14)
    CountLabel.Position = UDim2.new(0, 44, 0, 32)
    CountLabel.BackgroundTransparency = 1
    CountLabel.Text = "0 selected"
    CountLabel.Font = Enum.Font.Gotham
    CountLabel.TextSize = 11
    CountLabel.TextColor3 = CurrentTheme.SubText
    CountLabel.TextXAlignment = Enum.TextXAlignment.Left
    CountLabel.ZIndex = 17
    CountLabel.Parent = HeaderBtn

    local ChevronLabel = Instance.new("TextLabel")
    ChevronLabel.Size = UDim2.new(0, 30, 0, 30)
    ChevronLabel.Position = UDim2.new(1, -40, 0.5, -15)
    ChevronLabel.BackgroundTransparency = 1
    ChevronLabel.Text = "â€º"
    ChevronLabel.Font = Enum.Font.GothamBold
    ChevronLabel.TextSize = 20
    ChevronLabel.TextColor3 = CurrentTheme.SubText
    ChevronLabel.ZIndex = 17
    ChevronLabel.Parent = HeaderBtn

    -- Dropdown List (initially hidden)
    local DropList = Instance.new("ScrollingFrame")
    DropList.Size = UDim2.new(1, 0, 0, 0)
    DropList.Position = UDim2.new(0, 0, 0, 56)
    DropList.BackgroundColor3 = CurrentTheme.CardBG
    DropList.BackgroundTransparency = 0.15
    DropList.BorderSizePixel = 0
    DropList.ScrollBarThickness = 3
    DropList.ScrollBarImageColor3 = CurrentTheme.SubText
    DropList.ScrollBarImageTransparency = 0.5
    DropList.CanvasSize = UDim2.new(0, 0, 0, 0)
    DropList.Visible = false
    DropList.ClipsDescendants = true
    DropList.ZIndex = 18
    DropList.Parent = Container

    local DropListCorner = Instance.new("UICorner")
    DropListCorner.CornerRadius = UDim.new(0, 16)
    DropListCorner.Parent = DropList

    local DropListGlass = Instance.new("Frame")
    DropListGlass.Size = UDim2.new(1, 0, 1, 0)
    DropListGlass.BackgroundColor3 = CurrentTheme.Glass1
    DropListGlass.BackgroundTransparency = 0.6
    DropListGlass.BorderSizePixel = 0
    DropListGlass.ZIndex = 18
    DropListGlass.Parent = DropList

    local DropListGlassCorner = Instance.new("UICorner")
    DropListGlassCorner.CornerRadius = UDim.new(0, 16)
    DropListGlassCorner.Parent = DropListGlass

    local DropListLayout = Instance.new("UIListLayout")
    DropListLayout.Padding = UDim.new(0, 4)
    DropListLayout.Parent = DropList

    DropListLayout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        DropList.CanvasSize = UDim2.new(0, 0, 0, DropListLayout.AbsoluteContentSize.Y + 8)
    end)

    local function updateCount()
        local count = 0
        for _, v in pairs(selectedValues) do
            if v then count = count + 1 end
        end
        CountLabel.Text = count .. " selected"
        
        if config.OnChanged then
            config.OnChanged(selectedValues)
        end
    end

    local function createOption(value)
        local OptionBtn = Instance.new("TextButton")
        OptionBtn.Size = UDim2.new(1, -8, 0, 40)
        OptionBtn.BackgroundColor3 = CurrentTheme.CardBG
        OptionBtn.BackgroundTransparency = 0.3
        OptionBtn.BorderSizePixel = 0
        OptionBtn.Text = ""
        OptionBtn.AutoButtonColor = false
        OptionBtn.ZIndex = 19
        OptionBtn.Parent = DropList

        local OptCorner = Instance.new("UICorner")
        OptCorner.CornerRadius = UDim.new(0, 10)
        OptCorner.Parent = OptionBtn

        local OptLabel = Instance.new("TextLabel")
        OptLabel.Size = UDim2.new(1, -50, 1, 0)
        OptLabel.Position = UDim2.new(0, 10, 0, 0)
        OptLabel.BackgroundTransparency = 1
        OptLabel.Text = value
        OptLabel.Font = Enum.Font.Gotham
        OptLabel.TextSize = 13
        OptLabel.TextColor3 = CurrentTheme.Text
        OptLabel.TextXAlignment = Enum.TextXAlignment.Left
        OptLabel.ZIndex = 20
        OptLabel.Parent = OptionBtn

        local Checkbox = Instance.new("Frame")
        Checkbox.Size = UDim2.new(0, 20, 0, 20)
        Checkbox.Position = UDim2.new(1, -30, 0.5, -10)
        Checkbox.BackgroundColor3 = CurrentTheme.ToggleOff
        Checkbox.BorderSizePixel = 0
        Checkbox.ZIndex = 20
        Checkbox.Parent = OptionBtn

        local CheckCorner = Instance.new("UICorner")
        CheckCorner.CornerRadius = UDim.new(0, 5)
        CheckCorner.Parent = Checkbox

        local CheckMark = Instance.new("TextLabel")
        CheckMark.Size = UDim2.new(1, 0, 1, 0)
        CheckMark.BackgroundTransparency = 1
        CheckMark.Text = "âœ“"
        CheckMark.Font = Enum.Font.GothamBold
        CheckMark.TextSize = 16
        CheckMark.TextColor3 = Color3.fromRGB(255, 255, 255)
        CheckMark.Visible = false
        CheckMark.ZIndex = 21
        CheckMark.Parent = Checkbox

        if selectedValues[value] then
            Checkbox.BackgroundColor3 = CurrentTheme.ToggleOn
            CheckMark.Visible = true
        end

        OptionBtn.MouseButton1Click:Connect(function()
            selectedValues[value] = not selectedValues[value]
            
            if selectedValues[value] then
                Checkbox.BackgroundColor3 = CurrentTheme.ToggleOn
                CheckMark.Visible = true
            else
                Checkbox.BackgroundColor3 = CurrentTheme.ToggleOff
                CheckMark.Visible = false
            end
            
            updateCount()
        end)

        OptionBtn.MouseEnter:Connect(function()
            TweenService:Create(OptionBtn, TweenInfo.new(0.2), {
                BackgroundTransparency = 0.15
            }):Play()
        end)

        OptionBtn.MouseLeave:Connect(function()
            TweenService:Create(OptionBtn, TweenInfo.new(0.2), {
                BackgroundTransparency = 0.3
            }):Play()
        end)
    end

    local function refreshOptions(values)
        for _, child in pairs(DropList:GetChildren()) do
            if child:IsA("TextButton") then
                child:Destroy()
            end
        end
        
        for _, value in ipairs(values) do
            createOption(value)
        end
    end

    HeaderBtn.MouseButton1Click:Connect(function()
        isOpen = not isOpen
        
        if isOpen then
            DropList.Visible = true
            DropList.Size = UDim2.new(1, 0, 0, 0)
            
            local targetHeight = math.min(200, DropListLayout.AbsoluteContentSize.Y + 8)
            
            TweenService:Create(Container, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
                Size = UDim2.new(1, 0, 0, 56 + targetHeight)
            }):Play()
            
            TweenService:Create(DropList, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
                Size = UDim2.new(1, 0, 0, targetHeight)
            }):Play()
            
            TweenService:Create(ChevronLabel, TweenInfo.new(0.2), {
                Rotation = 90
            }):Play()
        else
            TweenService:Create(Container, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
                Size = UDim2.new(1, 0, 0, 56)
            }):Play()
            
            TweenService:Create(DropList, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {
                Size = UDim2.new(1, 0, 0, 0)
            }):Play()
            
            TweenService:Create(ChevronLabel, TweenInfo.new(0.2), {
                Rotation = 0
            }):Play()
            
            task.wait(0.3)
            DropList.Visible = false
        end
    end)

    return {
        SetValues = refreshOptions,
        SetValue = function(values)
            selectedValues = values or {}
            updateCount()
        end,
        GetValue = function()
            return selectedValues
        end
    }
end

